	<xs:complexType name="Receipt">
		<xs:annotation>
			<xs:documentation xml:lang="EN">expl: Generalized Receipt</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="HeaderText" form="qualified" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
