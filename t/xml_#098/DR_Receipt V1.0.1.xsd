					<xs:enumeration value="DINERS"/>
					<xs:enumeration value="DISC"/>
					<xs:enumeration value="JCB"/>
					<xs:enumeration value="MC"/>
					<xs:enumeration value="VISA"/>
					<xs:enumeration value="STORE"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="CustomerIdentification">
		<xs:annotation>
			<xs:documentation>ARTS: Associated with specific items, such as CheckTenderLineItem
            </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PersonalID" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ARTS: PersonalIDNumber </xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="PersonalIDType">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="DriverLicense"/>
										<xs:enumeration value="Passport"/>
										<xs:enumeration value="CreditCard"/>
										<xs:enumeration value="NationalIdentityCard"/>
										<xs:enumeration value="Telephone"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="BirthDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ARTS: BirthDate</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SignatureImage" type="xs:base64Binary" minOccurs="0">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TransactionReference">
		<xs:sequence>
			<xs:element name="TransactionID" type="NameToken"/>
			<xs:element name="RetailReference" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="MerchantID" type="NameToken">
							<xs:annotation>
								<xs:documentation>Receipt/Issuer/PartyID</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="RetailStoreID" type="NameToken">
							<xs:annotation>
								<xs:documentation>Receipt/Location/PartyID</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="WorkstationID" type="NameToken">
							<xs:annotation>
								<xs:documentation>Receipt/Device/DeviceID</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="TransactionNumber" type="xs:nonNegativeInteger">
							<xs:annotation>
								<xs:documentation>Receipt/Device/TransactionNumber</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="BusinessDayDate" type="xs:date">
							<xs:annotation>
								<xs:documentation>Receipt/Timestamp</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Time" type="xs:time" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Receipt/Timestamp</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Associate">
		<xs:sequence>
			<xs:element name="AssociateID" maxOccurs="unbounded">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="IDType" use="optional">
								<xs:simpleType>
									<xs:restriction base="NameToken">
										<xs:enumeration value="OperatorID"/>
										<xs:enumeration value="EmployeeID"/>
										<xs:enumeration value="SalespersonID"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:choice>
				<xs:element name="Name" type="xs:string" minOccurs="0"/>
				<xs:element name="PersonName" type="PersonName" minOccurs="0"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RetailTransaction">
		<xs:complexContent>
			<xs:extension base="TransactionDetail">
				<xs:sequence>
					<xs:element name="ShipTo" type="AddressedParty" minOccurs="0"/>
					<xs:element name="BillTo" type="AddressedParty" minOccurs="0"/>
					<xs:element name="Shipment" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Carrier" minOccurs="0">
									<xs:annotation>
										<xs:documentation>add ID attribute?</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:simpleContent>
											<xs:extension base="Text">
												<xs:attributeGroup ref="LinkGroup"/>
											</xs:extension>
										</xs:simpleContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="ShipmentTrackingID" minOccurs="0">
									<xs:annotation>
										<xs:documentation>use @href for tracking
                                        URL</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:simpleContent>
											<xs:extension base="NameToken">
												<xs:attribute name="href" type="xs:anyURI" use="optional"/>
											</xs:extension>
										</xs:simpleContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="ServiceLevel" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Element can contain free text.
                                            ServiceLevelCode enumerated from X12 284.
                                        </xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:simpleContent>
											<xs:extension base="Text">
												<xs:attribute name="ServiceLevelCode" type="ServiceLevelCode" use="optional"/>
											</xs:extension>
										</xs:simpleContent>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="Operator" type="Associate">
						<xs:annotation>
							<xs:documentation>ARTS: OperatorID</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Salesperson" type="Associate" minOccurs="0">
						<xs:annotation>
							<xs:documentation>ARTS: CommissionEmployeeID</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Total" maxOccurs="unbounded">
						<xs:complexType>
							<xs:annotation>
								<xs:documentation>ARTS: formerly included in RetailTransaction
                                </xs:documentation>
							</xs:annotation>
							<xs:sequence>
								<xs:element name="Text" type="Text" minOccurs="0"/>
								<xs:element name="Amount" type="Amount"/>
								<xs:element name="ItemCount" type="xs:integer" minOccurs="0"/>
							</xs:sequence>
							<xs:attribute name="TotalType">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Total"/>
										<xs:enumeration value="Subtotal"/>
										<xs:enumeration value="FoodStampTotal"/>
										<xs:enumeration value="TaxableTotal"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
					<xs:element name="CustomerLoyaltyAccount" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Text" type="Text" minOccurs="0"/>
								<xs:element name="CustomerAccountID" type="NameToken"/>
								<xs:element name="EffectiveDate" type="xs:date" minOccurs="0"/>
								<xs:element name="ExpirationDate" type="xs:date" minOccurs="0"/>
								<xs:element name="CumulativeSales" type="MonetaryAmount" minOccurs="0"/>
								<xs:element name="CumulativePoints" minOccurs="0" maxOccurs="unbounded">
									<xs:complexType>
										<xs:simpleContent>
											<xs:extension base="xs:integer">
												<xs:attribute name="PointsType" type="xs:NMTOKEN" use="optional"/>
											</xs:extension>
										</xs:simpleContent>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:attributeGroup ref="LinkGroup"/>
						</xs:complexType>
					</xs:element>
					<xs:element name="ReturnItem" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:complexContent>
								<xs:extension base="SaleItem">
									<xs:sequence>
										<xs:element name="OriginalTransaction" type="TransactionReference" minOccurs="0"/>
									</xs:sequence>
									<xs:attribute name="ReturnReasonCode" type="xs:NMTOKEN" use="optional"/>
								</xs:extension>
							</xs:complexContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="SaleItem" type="SaleItem" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>ARTS: SaleReturnLineItem </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GiftCertificate" type="GiftCertificateLineItem" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Discount" type="Discount" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>ARTS: DiscountLineItem </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Shipping" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Use for Shipping and related fees, such as Handling
                                and Insurance</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Text" type="Text" minOccurs="0"/>
								<xs:element name="Amount" type="Amount"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="Tax" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>ARTS: TaxLineItem </xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Text" type="Text" minOccurs="0"/>
								<xs:element name="Amount" type="Amount"/>
								<xs:element name="TaxableAmount" type="MonetaryAmount" minOccurs="0">
									<xs:annotation>
										<xs:documentation>ARTS: TaxableAmount </xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="TaxRate" type="Percentage" minOccurs="0"/>
								<xs:element name="TaxExemptAmount" minOccurs="0">
									<xs:complexType>
										<xs:simpleContent>
											<xs:extension base="MonetaryAmount">
												<xs:attribute name="TaxExemptionID" type="xs:string"/>
												<xs:attribute name="TaxExemptionReasonCode" type="xs:string"/>
											</xs:extension>
										</xs:simpleContent>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="TaxAuthorityType" type="TaxAuthority" use="optional"/>
							<xs:attribute name="TaxAuthorityID" type="xs:NMTOKEN" use="optional"/>
						</xs:complexType>
					</xs:element>
					<xs:element name="Tender" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>ARTS: TenderLineItem </xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:annotation>
								<xs:documentation>ARTS: TenderLineItem </xs:documentation>
							</xs:annotation>
							<xs:sequence>
								<xs:element name="Text" type="Text" minOccurs="0"/>
								<xs:element name="Amount" type="Amount"/>
								<xs:element name="ForeignCurrencyAmount" minOccurs="0">
									<xs:annotation>
										<xs:documentation>expl: Amount of "foreign" currency
                                            tendered. [A currency other than the base currency of
                                            the transaction.] </xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:simpleContent>
											<xs:extension base="xs:float">
												<xs:attribute name="CurrencyCode" type="CurrencyCode" use="required"/>
											</xs:extension>
										</xs:simpleContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="CustomerIdentification" type="CustomerIdentification" minOccurs="0"/>
								<xs:element name="Authorization" minOccurs="0">
									<xs:complexType>
										<xs:annotation>
											<xs:documentation>ARTS: Authorization
                                            </xs:documentation>
										</xs:annotation>
										<xs:sequence>
											<xs:element name="ProviderID" type="xs:string" minOccurs="0">
												<xs:annotation>
													<xs:documentation>ARTS:
                                                  PartyID:PartyRoleTypeCode -- the
                                                  authorization provider is a generalized
                                                  PartyRole in the ARTS
                                                  model</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="ApprovalCode" type="xs:string">
												<xs:annotation>
													<xs:documentation>ARTS: AdjudicationCode.  A
                                                  code returned in an authorization request,
                                                  and recorded as proof of authorization.
                                                  </xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="ReferenceID" type="NameToken" minOccurs="0">
												<xs:annotation>
													<xs:documentation>expl: typically a "Reference
                                                  Number," assigned to the transaction and
                                                  printed on the cardholder's statement to
                                                  identify the transaction in case of dispute
                                                  or inquiry.</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="ReasonCode" type="NameToken" minOccurs="0">
												<xs:annotation>
													<xs:documentation>expl:  indicates approval or
                                                  basic reason for denial. (Not all negative
                                                  responses will preclude acceptance of the
                                                  card as tender.)</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="ApprovalDateTime" type="xs:dateTime" minOccurs="0"/>
											<xs:element name="AddressVerification" minOccurs="0">
												<xs:complexType>
													<xs:attribute name="ShipTo" type="xs:boolean" use="optional"/>
													<xs:attribute name="BillTo" type="xs:boolean" use="optional"/>
													<xs:attribute name="PostalCode" type="xs:NMTOKEN" use="optional"/>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:choice minOccurs="0">
									<xs:element name="Cheque">
										<xs:annotation>
											<xs:documentation>ARTS:
                                            CheckTenderLineItem</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="BankID" type="xs:positiveInteger" minOccurs="0">
													<xs:annotation>
														<xs:documentation>ARTS: BankID
                                                  </xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ChequeAccountNumber" type="xs:positiveInteger">
													<xs:annotation>
														<xs:documentation>ARTS: CheckAccountNumber
                                                  </xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="ChequeNumber" type="xs:positiveInteger" minOccurs="0">
													<xs:annotation>
														<xs:documentation>ARTS: CheckSequenceNumber
                                                  </xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="Credit" type="PaymentCard">
										<xs:annotation>
											<xs:documentation>ARTS: Credit/DebitCardTenderLineItem
                                            </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Debit">
										<xs:annotation>
											<xs:documentation>ARTS: Credit/DebitCardTenderLineItem
                                            </xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="PaymentCard">
													<xs:sequence>
														<xs:element name="HostText" type="xs:string" minOccurs="0">
															<xs:annotation>
																<xs:documentation>ARTS: N/A.  ex:
                                                  (US) 12-CFR-205 Reg E Text from
                                                  Switch</xs:documentation>
															</xs:annotation>
														</xs:element>
													</xs:sequence>
												</xs:extension>
											</xs:complexContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="GiftCertificate" type="GiftCertificateTender">
										<xs:annotation>
											<xs:documentation>ARTS:
                                            GiftCertificateTenderLineItem</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="CustomerAccount">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="AccountNumber" type="AccountNumber"/>
												<xs:element name="BalanceForward" type="MonetaryAmount" minOccurs="0"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="PurchaseOrder">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="PurchaseOrderID" type="NameToken"/>
												<xs:element name="Date" type="xs:date" minOccurs="0"/>
												<xs:element name="AuthorizedAmount" type="MonetaryAmount" minOccurs="0"/>
												<xs:element name="Contact" type="Party"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element name="Coupon">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="CouponID" type="GTIN"/>
												<xs:element name="ExtendedCode" type="NameToken" minOccurs="0"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:choice>
							</xs:sequence>
							<xs:attribute name="TenderType" default="Cash">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Cash"/>
										<xs:enumeration value="Cheque"/>
										<xs:enumeration value="CreditCard"/>
										<xs:enumeration value="Debit"/>
										<xs:enumeration value="CustomerAccount"/>
										<xs:enumeration value="ChequeCard"/>
										<xs:enumeration value="GiftCertificate"/>
										<xs:enumeration value="TravelersCheque"/>
										<xs:enumeration value="Coupon"/>
										<xs:enumeration value="FoodStamp"/>
										<xs:enumeration value="WICCheque"/>
										<xs:enumeration value="PurchaseOrder"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attributeGroup ref="LinkGroup"/>
						</xs:complexType>
					</xs:element>
					<xs:element name="TenderChange" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>ARTS: TenderChangeLineItem </xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Text" type="Text" minOccurs="0"/>
								<xs:element name="Amount" type="Amount"/>
							</xs:sequence>
							<xs:attribute name="TenderType" default="Cash">
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Cash"/>
										<xs:enumeration value="FoodStamp"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
					<xs:element name="LoyaltyPointsAwarded" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Text" type="Text" minOccurs="0"/>
								<xs:element name="PointsAwarded" type="xs:integer"/>
							</xs:sequence>
							<xs:attribute name="PointsType" type="xs:NMTOKEN"/>
							<xs:attributeGroup ref="LinkGroup"/>
						</xs:complexType>
					</xs:element>
					<xs:element name="TrailerText" minOccurs="0">
						<xs:complexType>
							<xs:simpleContent>
								<xs:extension base="Text">
									<xs:attributeGroup ref="LinkGroup"/>
								</xs:extension>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="PromotionText" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Text" type="Text" minOccurs="0"/>
								<xs:element name="StructuredText" type="StructuredText" minOccurs="0"/>
							</xs:sequence>
							<xs:attributeGroup ref="LinkGroup"/>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>
