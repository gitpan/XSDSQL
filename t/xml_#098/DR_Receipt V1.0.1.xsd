			<xs:element name="Issuer" type="Party" form="qualified" minOccurs="0"/>
			<xs:element name="Recipient" type="Party" form="qualified" minOccurs="0"/>
			<xs:element name="CopyTo" type="Party" form="qualified" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Location" type="Party" form="qualified"/>
			<xs:element name="Device" form="qualified" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="DeviceID" type="xs:string" form="qualified">
							<xs:annotation>
								<xs:documentation xml:lang="EN">ARTS:
                                WorkstationID</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="TransactionNumber" type="xs:nonNegativeInteger" form="qualified">
							<xs:annotation>
								<xs:documentation xml:lang="EN">ARTS: TransactionSequenceNumber
                                </xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="DeviceType" form="unqualified">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="POS"/>
								<xs:enumeration value="CallCenterStation"/>
								<xs:enumeration value="Network"/>
								<xs:enumeration value="ATM"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="TransactionID" type="NameToken" form="qualified">
