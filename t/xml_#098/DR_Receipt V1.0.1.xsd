				<xs:complexType>
					<xs:sequence>
						<xs:element name="TransactionID" type="NameToken" form="qualified"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Language" type="xs:language" use="optional" form="unqualified"/>
		<xs:attribute name="Currency" type="CurrencyCode" use="optional" form="unqualified"/>
	</xs:complexType>
